<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.to-doList" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>To-do List</name>
    <description>
        To-do List to keep track of your daily work.
    </description>
    <author email="pooja.priya9653@gmail.com" href="http://cordova.io">
       justaHuman
    </author>
    <icon src="index.img" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
